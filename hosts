# alkawsar
[staging]
staging.alkawsar.com ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/alkawsar-staging.cer

[production]
www.alkawsar.com ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/alkawsar-website-prod.cer

